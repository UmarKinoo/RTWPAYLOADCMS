{
  "name": "payload-starter",
  "version": "1.0.0",
  "description": "A saas starter template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation pnpm generate:types && cross-env NODE_OPTIONS=--no-deprecation next build --webpack",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --webpack",
    "devsafe": "node scripts/cleanup.js && cross-env NODE_OPTIONS=--no-deprecation next dev --webpack",
    "cleanup": "powershell -ExecutionPolicy Bypass -File scripts/cleanup-windows.ps1",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "seed:skills": "cross-env NODE_OPTIONS=--no-deprecation tsx src/scripts/seed-skills.ts",
    "seed:plans": "cross-env NODE_OPTIONS=--no-deprecation tsx src/scripts/seed-plans.ts",
    "seed:disciplines": "cross-env NODE_OPTIONS=--no-deprecation tsx src/scripts/seed-disciplines.ts",
    "translate:disciplines": "cross-env NODE_OPTIONS=--no-deprecation tsx src/scripts/translate-disciplines.ts",
    "optimize:images": "tsx scripts/optimize-images.ts",
    "translate:ar": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/generate-arabic-translations.ts",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  },
  "dependencies": {
